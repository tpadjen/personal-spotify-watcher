{
  "name": "spotify-current-song-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "initialize": "mkdir data",
    "clean": "del-cli \"dist/*.*\"",
    "tsc": "./node_modules/typescript/bin/tsc",
    "build": "npm run tsc",
    "prebuild:prod": "del-cli dist",
    "build:prod": "./node_modules/typescript/bin/tsc --outDir dist --sourceMap false",
    "postbuild:prod": "npm run importclient",
    "importclient": "del-cli dist/client && cp -r ../current-client/dist dist/client",
    "dev": "tsc-watch --onSuccess \"node src/index.js\"",
    "prestart": "npm run build",
    "start": "node src/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Tim Padjen",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.18",
    "@types/ws": "^7.2.6",
    "bufferutil": "^4.0.1",
    "del-cli": "^3.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lowdb": "^1.0.0",
    "spotify-web-api-node": "^4.0.0",
    "typescript": "^3.9.6",
    "utf-8-validate": "^5.0.2",
    "ws": "^7.3.1"
  },
  "engines": {
    "node": "14.x"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cpy-cli": "^3.1.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.10.2",
    "tsc-watch": "^4.2.9"
  }
}
